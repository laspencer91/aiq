{
  "name": "aiq-cli",
  "author": "Logan Spencer",
  "version": "0.1.3",
  "description": "A lightweight CLI tool for reusable AI prompts",
  "main": "dist/index.js",
  "bin": {
    "aiq": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "release": "tsc && npm version patch && npm publish",
    "release:minor": "tsc && npm version minor && npm publish",
    "release:major": "tsc && npm version major && npm publish",
    "release:preview": "tsc && npm publish --dry-run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "ai",
    "cli",
    "gemini",
    "prompt",
    "terminal",
    "productivity"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.1",
    "inquirer": "^9.2.15",
    "ora": "^8.0.1",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aiq.git"
  }
}
